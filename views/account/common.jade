block config
doctype html
html(lang="zh-CN")
    head
        meta(charset="utf-8")
        meta(http-equiv="X-UA-Compatible",content="IE=edge")
        meta(name="viewport",content="width=device-width, initial-scale=1")
        //上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！
        title= seo.title
        meta(name="keywords",content="#{seo.keywords}")
        meta(name="description",content="#{seo.discription}")
        meta(name="author",content="jintoushou")
        link(rel='shortcut icon',href='/favicon.ico')
        link(rel='stylesheet', href='/stylesheets/font-awesome.min.css',id='fontawesomecss')
        link(rel='stylesheet', href='/css/jintoushou-main.min.css')
        block style
    body
        block content
        block header
            header#header
                div.container.top-bar
                    div.row
                        div.col-40 服务热线：400-101-7660
                            .fa.fa-weixin
                                .code
                                    i.fa.fa-caret-up
                                    img(src='/images/codeWeixin.png',width='106',alt='winxin')
                                    span 扫一扫关注金投手
                            a(href='http://weibo.com/jtoushou',target='_blank')
                                i.fa.fa-weibo
                        div.col-60
                            if tel
                                a(href='/user/accountoverview.html') 欢迎您，#{tel}
                                a(href='/logout.html') 退出
                                a.info(href='#') 消息
                                    sup 12
                            else
                                a(href='/signin.html?source=1') 登录
                                a(href='/signup.html') 注册
                            a(href='#') 新手引导
                            a(href='/help/help.html') 帮助中心
                div.navbar
                    div.container
                        div.row
                            div.col-35
                                h1.hidden 金投手金融信息服务(北京)有限公司
                                a.logo(href='/') 金投手 保收益
                            div.col-65
                                nav
                                    ul
                                        - var narBarInfo=[{title:'首页',url:'/'},{title:'保理',url:'/product/factoring.html'},{title:'出口退税',url:'/product/taxrebate.html'},{title:'当前在售',url:'/product/currentsale.html'},{title:'投资者教育',url:'投资者教育.html',target:'_blank'},{title:'信息披露',url:'信息披露.html',target:'_blank'},{title:'我的账户',url:'/user/accountoverview.html'}];
                                        each item in narBarInfo
                                            li(class=(item.title === curUrlTitle) ? 'active' : '')
                                                if item.title == '我的账户'
                                                    a.account(href=item.url)
                                                        span #{item.title}
                                                else
                                                    a(href=item.url,target=item.target) #{item.title}
        block position
        if position
            //面包屑导航开始
            .container.breadcrumb 您所在的位置：
                ol
                    - breadcrumbInfo=[{title:'首页',url:'/'},{title:'我的账户',url:'/account/accountoverview.html'},{title:'账户总览',url:''}];
                    each item,i in breadcrumbInfo
                        if(i == breadcrumbInfo.length - 1)
                            li #{item.title}
                        else
                            li
                                a(href=item.url) #{item.title}
        //面包屑导航结束

        block leftnav
        if leftnav
            //列表页左侧导航
            section.container

                .left_nav
                    -
                        listnav=[
                        {
                        children:[
                        {name:'公司介绍',url:'/xxpl/gsjs',active:true},
                        {name:'团队介绍',url:'/xxpl/tdjs.html'},
                        {name:'运营数据',url:'/xxpl/yysj.html'},
                        {name:'通知公告',url:'/xxpl/tzgg.html'},
                        {name:'公司报告',url:'/xxpl/gsbg.html'},
                        {name:'公司动态',url:'/xxpl/gsdt.html'},
                        {name:'媒体报道',url:'/xxpl/mtbg.html'},
                        {name:'合作伙伴',url:'/xxpl/hzhb.html'},
                        {name:'联系我们',url:'/xxpl/lxwm.html'},
                        ]
                        },
                        ]
                    each items in listnav
                        ul
                            each item in items.children
                                li(class=item.hasOwnProperty('active') === true ? 'active ' + item.active : '')
                                    a(href=item.url) #{item.name}
                block right_help

            //列表页左侧导航


        block leftnavtzzjy
        if leftnavtzzjy
            //投资者列表页左侧导航
            section.container
                .left_nav
                    -
                        listnav=[
                        {
                        children:[
                        {name:'相关法律',active:true},
                        {name:'行政法规'},
                        {name:'规范性文件'},
                        {name:'司法解释'},
                        {name:'知识百科'},
                        {name:'专栏文章'},
                        ]
                        },
                        ]
                    each items in listnav
                        ul
                            each item in items.children
                                li(class=item.hasOwnProperty('active') === true ? 'active ' + item.active : '')
                                    a(href=item.url) #{item.name}
                block right_help
        block left
        if left
            //主体内容开始
            section.container
                //列表目录
                aside#aside
                    .list_group
                        -
                            listInfo=[
                            {
                            title:'我的账户',
                            children:[
                            {name:'账户总揽',url:'/user/accountoverview.html'},
                            {name:'我的项目',url:'/user/myrepayment.html'}

                            ]
                            },
                            {
                            title:'资金管理',
                            children:[
                            {name:'交易记录',url:'/user/dealhistory.html'},
                            {name:'我要充值',url:'/user/recharge.html'},
                            {name:'我要提现',url:'/user/getcash.html'},
                            {name:'银行卡信息',url:'/2'},
                            {name:'银行存管账户信息',url:'/user/bankaccountinfo.html'}
                            ]
                            },
                            {
                            title:'我的奖励',
                            children:[
                            {name:'抵现红包',url:'/user/redpacket.html'},
                            {name:'加息券',url:'/user/ratecoupon.html'},
                            ]
                            },
                            {
                            title:'邀请管理',
                            children:[
                            {name:'邀请好友',url:'/user/invitedfriends.html'},
                            {name:'我的好友',url:'/user/myfriends.html'}
                            ]
                            },
                            {
                            title:'账户设置',
                            children:[
                            {name:'安全设置',url:'/user/securitysetup.html'},
                            {name:'消息公告',url:'/2'}
                            ]
                            }
                            ]
                        each items in listInfo
                            h4 #{items.title}
                                i.fa.fa-caret-down.fa-fw
                            ol
                                each item in items.children
                                    li(class=(item.name === curUrlTitle) ? 'active' : '')

                                        a(href=item.url) #{item.name}
                block right
        block main
block footer
