//工具栏
.toolbar
    ul
        li.mobile_btn
            .weixin_code
                i
                    img(src='https://www.jintoushou.com/source/v1/pc/img/codeWeixin.png' style='margin-top:-4px;')
                p 关注金投手
                span.angle
        a(href='http://www.sobot.com/chat/pc/index.html?sysNum=700b7ec5c8884e6fbc16baedf679ceca',target='_blank')
            li.online_service
        li.calculator_btn
            dl.calculator
                dt 理财计算器
                    a(href='javascript:;') &times;
                dd
                    form
                        .form_label
                            div.form-error
                                i.text 错误
                                a(href="javascript:;").btn-close ×
                                i.fa.fa-caret-down.fa-w
                            input.form_input#amount(placeholder='输入投资金额')
                            span 元
                        .color-gray.text-center#unitType 计息单位&nbsp;&nbsp;&nbsp;
                            label
                                input(type='radio',name='radio',value='month')
                                span &nbsp;月&nbsp;&nbsp;
                            label
                                input(type='radio',name='radio',value='day',checked)
                                span &nbsp;天
                        .form_label
                            div.form-error
                                i.text 错误
                                a(href="javascript:;").btn-close ×
                                i.fa.fa-caret-down.fa-w
                            input.form_input#term(placeholder='输入投资期限')
                            span 天
                        .form_label
                            div.form-error
                                i.text 错误
                                a(href="javascript:;").btn-close ×
                                i.fa.fa-caret-down.fa-w
                            input.form_input#annualRate(placeholder='输入年化收益')
                            span %
                        .form_label
                            select.form_input#repayType
                                option(selected,value='principalAndInterestTotally') 一次性还本息
                                option#unitTypeDay(value='equalInterestMonthlyByDay')  先息后本
                        div
                            input.btn_reset(type='reset',value='重置')
                            a.btn_submit(href='javascript:;') 开始计算
            .calculator_result
                .leftBar
                    a.fa.fa-angle-right(href='javascript:;')
                .resultTable
                    //p 假设起息日期：2016-00-00
                    table.table(width='100%',cellpadding='0',cellspacing='0')
                        thead
                            tr
                                td(style='border-left:none;') 还款日期
                                td 应收本息（元）
                                td 应收利息（元）
                                td 应收本金（元）
                        tbody#repayPlanBody
                            tr.color-gray
                                td(style='border-left:none;') 2016-04-01
                                td 0000.00
                                td 0000.00
                                td 0000.00
                        tfoot#repayPlanFooter
                            tr
                                th(style='border-left:none;') 总计
                                th 00.00
                                th 00.00
                                th 0000.00
        li.app_btn_none
            .app_code
                i
                    img(src='https://www.jintoushou.com/source/v1/pc/img/qrcode.jpg')
                p 扫描下载APP
                span.angle
        li.gotop_btn
